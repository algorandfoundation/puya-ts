subroutine test_uint64(x: uint64, y: uint64): bool
{
  x++
  x--
  var x: uint64 = (y = y - 1)
  var x: uint64 = (y = y + 1)
  var y: uint64 = ~y
  return !y
}
subroutine test_biguint(x: biguint, y: biguint): bool
{
  x++
  x--
  var x: biguint = (y = y - 1)
  var x: biguint = (y = y + 1)
  return !bitlen(y)
}
subroutine test_bytes(x: bytes): void
{
  var y: bytes = ~x
}