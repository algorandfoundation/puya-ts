subroutine test_uint64(x: uint64, y: uint64): bool
{
  x++
  x--
  (x = ((y = y - 1)))
  (x = ((y = y + 1)))
  (y = ~y)
  return !y
}
subroutine test_biguint(x: biguint, y: biguint): bool
{
  x++
  x--
  (x = ((y = y - 1)))
  (x = ((y = y + 1)))
  return !bitlen(y)
}
subroutine test_bytes(x: bytes): void
{
  var y: bytes = ~x
}