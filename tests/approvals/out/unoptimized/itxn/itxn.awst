  test4(): void
  {
    lotsOfBytes: bytes[2044] = bzero(2044)
    approval2: bytes = concat(concat(0x80fc0f, reinterpret_cast<bytes>(lotsOfBytes)), 0x48)
    appTxn1: inner_transaction_fields_appl = create_inner_transaction(Fee=0, TypeEnum=6, ApprovalProgramPages=#[0x098101, approval2, approval2, approval2], ClearStateProgramPages=#[0x098101], OnCompletion=5, ApplicationArgs=#["1"], ExtraProgramPages=3)
    app1: inner_transaction_appl = submit_txn(appTxn1)
    assert(app1.ExtraProgramPages == 3, comment=extra pages = 3)
    assert(app1.NumApprovalProgramPages == 2, comment=approval pages = 2)
    assert(app1.ApprovalProgramPages[0] == concat(concat(0x098101, approval2), approval2.slice(0, -3)), comment=expected approval page 0)
    assert(app1.ApprovalProgramPages[1] == concat(approval2.slice(-3), approval2), comment=expected approval page 1)
    assert(app1.NumClearStateProgramPages == 1, comment=clear state pages = 1)
    assert(app1.ClearStateProgramPages[0] == 0x098101, comment=expected clear state page)
  }
  
  test5(appId: application): void
  {
    pay: inner_transaction_fields_pay = create_inner_transaction(Fee=0, TypeEnum=1, Receiver=checked_maybe(app_params_get<AppAddress>(appId), comment=application exists), Amount=1000)
    app: inner_transaction_fields_appl = create_inner_transaction(Fee=0, TypeEnum=6, ApplicationID=appId, ApplicationArgs=#[Method(name="receivePay",args=(uint64,group_transaction_pay,string),result=arc4.tuple<uint64,uint64,string>,resource_encoding="value"), 0x0000000000000001, reinterpret_cast<bytes>("hello")])
    #[_, appRes]: readonly [inner_transaction_pay, inner_transaction_appl] = submit_txn(pay, app)
    returnValue: arc4.tuple<uint64,uint64,string> = ARC4_DECODE(reinterpret_cast<arc4.tuple<arc4.uint64,arc4.uint64,arc4.dynamic_array<arc4.byte>>>(checked_maybe(#[extract<4, 0>(($0 = appRes.LastLog)), extract<0, 4>($0) == 0x151f7c75], comment=Bytes has valid prefix)))
    assert(&&(&&(&&(3 == 3, returnValue.0 == 1), returnValue.1 == 1000), returnValue.2 == "hello"), comment=assert target is match for conditions)
  }
  
  __algots__.defaultCreate(): void
  {
  }
  
  constructor(): void
  {
    super.constructor()
    GlobalState["name"]: bytes = 0x
  }
  
  BaseContract::constructor(): void
  {
  }
  
}
contract ItxnReceiver
{
  approvalProgram(): bool
  {
    return arc4Router()
  }
  
  clearProgram(): bool
  {
    return True
  }
  
  receivePay(a: uint64, b: group_transaction_pay, c: string): arc4.tuple<uint64,uint64,string>
  {
    return ARC4_ENCODE(#[a, gtxns<Amount>(b), c], wtype=arc4.tuple<uint64,uint64,string>)
  }
  
  __algots__.defaultCreate(): void
  {
  }
  
}