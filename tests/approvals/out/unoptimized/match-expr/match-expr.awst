contract MatchExprAlgo
{
  approvalProgram(): bool
  {
    return arc4Router()
  }
  
  clearProgram(): bool
  {
    return True
  }
  
  testMatches(x: uint64): void
  {
    xObj: {x:uint64} = new {x:uint64}(x=($0 = #{ x: x }).x)
    assert(xObj.x == 5, comment=x should be 5)
    assert(xObj.x != 3, comment=x should not be 3)
    assert(txn<Sender>() != global<ZeroAddress>(), comment=assert target is match for conditions)
    assert(xObj.x > 4)
    assert(xObj.x < 6)
    assert(xObj.x >= 5)
    assert(xObj.x <= 5)
    xArray: arc4.dynamic_array<uint64> = new arc4.dynamic_array<uint64>(($1 = #[x]).0)
    assert(xArray.length == 1 and xArray[0] == 5)
    assert(xArray.length == 1 and xArray[0] > 4)
    assert(xArray.length == 1 and xArray[0] < 6)
    assert(xArray.length == 1 and xArray[0] >= 5)
    assert(xArray.length == 1 and xArray[0] <= 5)
  }
  
  __algots__.defaultCreate(): void
  {
  }
  
}