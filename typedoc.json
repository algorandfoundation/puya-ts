{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./packages/algo-ts/src/index.ts",
    "./packages/algo-ts/src/op.ts",
    "./packages/algo-ts/src/itxn.ts",
    "./packages/algo-ts/src/gtxn.ts",
    "./packages/algo-ts/src/arc4/index.ts"
  ],
  "exclude": ["./packages/algo-ts/src/internal.ts", "./node_modules/typescript/**/*.ts"],
  "outputs": [
    {
      "name": "html",
      "path": "docs/_html"
    }
  ],
  "projectDocuments": ["docs/language-guide.md", "docs/cli.md", "docs/reference.md", "docs/migration-guides.md"],
  "plugin": ["typedoc-plugin-missing-exports", "typedoc-plugin-markdown"],
  "name": "Algorand TypeScript",
  "cleanOutputDir": true,
  "excludeReferences": true,
  "includeHierarchySummary": false,
  "readme": "docs/readme.md",
  "gitRevision": "main",
  "validation": {
    "notDocumented": true
  }
}
