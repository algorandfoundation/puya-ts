{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./packages/algo-ts/src/index.ts", "./packages/algo-ts/src/op.ts", "./packages/algo-ts/src/arc4/index.ts"],
  "exclude": ["./packages/algo-ts/src/internal.ts"],
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-missing-exports"],
  "cleanOutputDir": true,
  "excludeReferences": true,
  "includeHierarchySummary": false,
  "readme": "none",
  "gitRevision": "main",
  "validation": {
    "notDocumented": true
  }
}
