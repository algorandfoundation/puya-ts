  "compilerOptions": {
    "noEmit": true,
    "lib": [
      "ES2023"
    ],
    "target": "ES2023",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts","rollup.config.ts"]
}
